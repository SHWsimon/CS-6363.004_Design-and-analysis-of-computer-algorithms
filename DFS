DFS(Depth-Fist search)

//way1
DFS(V)
  //V{n} is node set
  //Initialization
  for u=V{n} 
    u.parent=null
    u.color=white
  t=0
  
  //main
  for u=V{n}
    if u.color==white
      DFS_visit(u)
      
DFS_visit(u)
  u.color=gray
  u.dis=t++
  for v=u.adj
    if v.color=white
      v.parent=u
      DFS_visit(v)
  u.color=black
  u.fin=t++
  
//way2: topology order
DFS(V)
  //V{n} is node set
  //Initialization
  for u=V{n} 
    u.parent=null
    u.color=white
  t=0, toporder={}, topnumber=|V|
  
  //main
  for u=V{n}
    if u.color==white
      DFS_visit(u)
      
DFS_visit(u)
  u.color=gray
  for v=u.adj
    if v.color=white
      v.parent=u
      DFS_visit(v)
  toporder[topnumber]=u
  u.top=topnumber--
  u.color=black
